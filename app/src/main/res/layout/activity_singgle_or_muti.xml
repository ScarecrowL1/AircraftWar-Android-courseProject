<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".singgleOrMuti">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="1491dp"
        android:layout_height="1741dp"
        android:contentDescription="backgrond"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.449"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.48"
        app:srcCompat="@mipmap/aircraft" />

    <Button
        android:id="@+id/mutiButton"
        android:layout_width="114dp"
        android:layout_height="84dp"
        android:layout_marginTop="60dp"
        android:text="@string/DuorenMoshi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/singleButton"
        app:layout_constraintVertical_bias="0.059" />

    <Button
        android:id="@+id/singleButton"
        android:layout_width="114dp"
        android:layout_height="84dp"
        android:layout_marginTop="236dp"
        android:text="@string/DanrenMoshi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>